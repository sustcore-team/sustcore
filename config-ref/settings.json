{
    "fileHeaderComment.parameter":{
        "*":{
            "author": "author",
            "lic_sustcore": [
                " Sustcore",
                " ",
                " 作者: ${author}",
                " ",
                " ${filename}",
                " ",
                " 在此处键入描述",
                " "
            ]
        }
    },
    "fileHeaderComment.template":{
        "header":[
            "${commentbegin}",
            "${commentprefix}${lic_sustcore}",
            "${commentend}",
            "",
            "",
            ""
        ]
    },
    "C_Cpp.default.systemIncludePath": [
        "${workspaceFolder}/"
    ],
    "doxdocgen.file.versionTag": "@version alpha-1.0.0",
    "doxdocgen.generic.authorName": "author",
    "doxdocgen.generic.authorEmail": "email",
    "files.associations": {
        "*.S": "riscv",
        "*.h": "cpp",
        "type_trait": "cpp",
        "concept": "cpp",
        "*.c": "c",
        "*.inc": "c",
        "*.idef": "c",
        "*.def": "c",
        "*.code": "c"
    },
    "clangd.path": "clangd",
    "clangd.arguments": [
        "--all-scopes-completion",
        "--background-index",
        "--clang-tidy",
        "--clang-tidy-checks=performance-*, bugprone-*, misc-*, google-*, modernize-*, readability-*, portability-*",
        "--compile-commands-dir=${workspaceFolder}/.vscode/",
        "--completion-parse=auto",
        "--completion-style=detailed",
        "--enable-config",
        "--function-arg-placeholders=true",
        "--header-insertion-decorators",
        "--header-insertion=iwyu",
        "--include-cleaner-stdlib",
        "--log=verbose",
        "--pch-storage=memory",
        "--pretty",
        "--ranking-model=decision_forest",
        "--query-driver=riscv64-unknown-elf-g++",
        "-j=12"
    ],
    "clangd.onConfigChanged": "restart",
    "clangd.serverCompletionRanking": true,
    "clangd.detectExtensionConflicts": true,
    "editor.suggest.snippetsPreventQuickSuggestions": true,
    "GitCommitPlugin.MaxSubjectCharacters": 40
}