#pragma once

namespace std {
    template <class T>
    void swap(T& a, T& b) {
        T temp = static_cast<T&&>(a);
        a      = static_cast<T&&>(b);
        b      = static_cast<T&&>(temp);
    }

    // template <typename T>
    // struct forward {};

}  // namespace std